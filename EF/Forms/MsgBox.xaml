<Window x:Class="EF.Forms.MsgBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EF.Forms"
        mc:Ignorable="d"
        Title="MsgBox" Height="301.838" Width="353.39"
        Loaded="OnLoad"
        WindowStartupLocation="CenterOwner">

    <Grid>
        <Label x:Name="lblMsg" Content="Label" HorizontalAlignment="Left" Margin="20,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnLeft" IsDefault="True" Content="Yes" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,65,0,0" Click="btnLeft_Click"/>
        <Button x:Name="btnRight" IsDefault="True" Content="No" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="95,65,0,0" RenderTransformOrigin="-0.075,-2.989" Click="btnRight_Click"/>
    </Grid>
</Window>
